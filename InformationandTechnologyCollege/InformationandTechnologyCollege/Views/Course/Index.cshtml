@model List<InformationandTechnologyCollege.Models.Course>

@{
    ViewBag.Title = "Index";
}

<h2>Courses</h2>

<div>
    <h3>Course 1: Computer Science</h3>
    <ul>
        @foreach (var course1 in @Model)
        {
            <li>
                <a href="~/Course/Details/@course1.CourseId">@course1.Name</a>
            </li>
        }
    </ul>
</div>